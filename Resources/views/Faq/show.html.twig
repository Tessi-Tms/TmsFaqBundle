{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Faq</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Enabled</th>
                <td>{{ entity.enabled }}</td>
            </tr>
            <tr>
                <th>Displayfromrules</th>
                <td>{{ entity.displayFromRules }}</td>
            </tr>
            <tr>
                <th>Customerid</th>
                <td>{{ entity.customerId }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('faq') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('faq_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
