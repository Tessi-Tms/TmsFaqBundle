{
    "name": "tms/faq-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle to manage Faq",
    "keywords": ["bundle", "faq" , "tessi"],
    "homepage": "https://github.com/Tessi-Tms/TmsFaqBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "TESSI MARKETING",
            "email": "contact@tessi.fr",
            "homepage": "http://www.tessi.fr"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Tessi-Tms/TmsRestBundle.git"
        }
    ],
    "require": {
        "php":                              ">=5.3.3",
        "symfony/framework-bundle":         ">=2.3",
        "doctrine/orm":                     "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle":         "~1.2",
        "twig/twig":                        "~1.0",
        "idci/simple-metadata-bundle":      "~1.0",
        "friendsofsymfony/elastica-bundle": "~5.0",
        "tms/rest-bundle":                  "dev-master"
    },
    "autoload": {
        "psr-0": { "Tms\\Bundle\\FaqBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Tms/Bundle/FaqBundle"
}
